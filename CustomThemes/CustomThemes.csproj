<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="SplashScreen\Image\shuttle.png" />
    <None Remove="SplashScreen\Image\test.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="SplashScreen\Image\shuttle.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="SplashScreen\Image\test.png" />
  </ItemGroup>

</Project>
